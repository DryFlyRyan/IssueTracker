  "keywords": [],
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "setupFiles": [
      "<rootDir>/test/config/enzymeTestAdapterSetup.js"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/config",
      "<rootDir>/demo-server",
      "<rootDir>/public",
      "<rootDir>/scripts",
      "<rootDir>/test",
      "<rootDir>/src/app/Router",
      "actions.js",
      "mockData.js",
      "index.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/config",
      "<rootDir>/demo-server",
      "<rootDir>/public",
      "<rootDir>/scripts",
      "<rootDir>/test"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".*": "<rootDir>/node_modules/jest-css-modules",
      "^.+\\.svg$": "<rootDir>/test/xformsAndMocks/svgTransform.js"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/xformsAndMocks/fileMock.js",
      "^testUtils(.*)$": "<rootDir>/src/utils/testUtils",
      "^stateData(.*)$": "<rootDir>/src/state$1",
      "^stateMocks(.*)$": "<rootDir>/src/state/_mocks/index.js",
      "^constants(.*)$": "<rootDir>/src/constants$1",
      "^actionCreators": "<rootDir>/src/data/actionCreators.js",
      "^actionConstants(.*)$": "<rootDir>/src/constants/actions/index.js",
      "^stringConstants(.*)$": "<rootDir>/src/constants/strings.js",
      "^commonStyles(.*)$": "<rootDir>src/app/app.styles.js",
      "^components(.*)$": "<rootDir>/src/app/components$1"
    },
    "testMatch": [
      "<rootDir>/src/**/*.unit.spec.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@cake)"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
